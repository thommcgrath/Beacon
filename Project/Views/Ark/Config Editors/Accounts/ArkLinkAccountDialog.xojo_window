#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub Constructor(Tokens() As BeaconAPI.ProviderToken)
		  Self.mTokens = Tokens
		  Super.Constructor
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Function Present(Parent As DesktopWindow, Tokens() As BeaconAPI.ProviderToken) As BeaconAPI.ProviderToken
		  If (Parent Is Nil) = False Then
		    Parent = Parent.TrueWindow
		  End If
		  
		  Var Win As New ArkLinkAccountDialog(Tokens)
		  Win.ShowModal(Parent)
		  Var SelectedToken As BeaconAPI.ProviderToken = Win.mSelectedToken
		  Win.Close
		  Return SelectedToken
		End Function
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mSelectedToken As BeaconAPI.ProviderToken
	#tag EndProperty

	#tag Property, Flags = &h21
		Private mTokens() As BeaconAPI.ProviderToken
	#tag EndProperty


#tag EndWindowCode

#tag Events NewConnectionButton
	#tag Event
		Sub Pressed()
		  Self.mSelectedToken = Nil
		  System.GotoURL(Beacon.WebURL("/account/#oauth", True))
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ActionButton
	#tag Event
		Sub Pressed()
		  If Self.TokensMenu.SelectedRowIndex = -1 Then
		    Return
		  End If
		  
		  Var Token As BeaconAPI.ProviderToken = Self.TokensMenu.SelectedRowTag
		  Self.mSelectedToken = Token
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Pressed()
		  Self.mSelectedToken = Nil
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TokensMenu
	#tag Event
		Sub Opening()
		  For Each Token As BeaconAPI.ProviderToken In Self.mTokens
		    Me.AddRow(Token.Label(BeaconAPI.ProviderToken.DetailHigh), Token)
		  Next
		  
		  If Me.RowCount = 1 Then
		    Me.SelectedRowIndex = 0
		  End If
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged(item As DesktopMenuItem)
		  Self.ActionButton.Enabled = (Item Is Nil) = False
		End Sub
	#tag EndEvent
#tag EndEvents
