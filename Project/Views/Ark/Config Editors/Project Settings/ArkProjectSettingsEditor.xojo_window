		Sub Change()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Project.UWPMode = CType(Me.SelectedRowIndex, Ark.Project.UWPCompatibilityModes)
		  Self.Changed = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
