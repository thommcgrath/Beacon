		Private Sub ServersEditor_ShouldDeployProfiles(Sender As SDTDServersEditor, SelectedProfiles() As Beacon.ServerProfile)
		  #Pragma Unused Sender
		  Self.BeginDeploy(SelectedProfiles)
		End Sub
