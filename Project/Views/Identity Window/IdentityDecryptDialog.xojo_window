#tag WindowCode
	#tag Event
		Sub Opening()
		  Self.SwapButtons()
		  Self.ActionButton.Enabled = False
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Shared Function Present(Parent As DesktopWindow) As String
		  If Parent Is Nil Then
		    Return ""
		  End If
		  
		  Var Win As New IdentityDecryptDialog
		  Win.ShowModal(Parent)
		  Var Password As String = Win.mPassword
		  Win.Close
		  Return Password
		End Function
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mPassword As String
	#tag EndProperty


#tag EndWindowCode

#tag Events ActionButton
	#tag Event
		Sub Pressed()
		  // No trim. Trailing or leading whitespace is perfectly valid for the password.
		  Self.mPassword = Self.PasswordField.Text
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Pressed()
		  Self.mPassword = ""
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PasswordField
	#tag Event
		Sub TextChanged()
		  // No trim. Trailing or leading whitespace is perfectly valid for the password.
		  Self.ActionButton.Enabled = Me.Text <> ""
		End Sub
	#tag EndEvent
#tag EndEvents
