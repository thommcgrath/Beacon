#tag WindowCode
	#tag Event
		Sub Close()
		  If Self = mInstance Then
		    mInstance = Nil
		  End If
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Shared Function SharedInstance(Create As Boolean = True) As DocumentManagerWindow
		  If mInstance = Nil And Create = True Then
		    mInstance = New DocumentManagerWindow
		  End If
		  Return mInstance
		End Function
	#tag EndMethod


