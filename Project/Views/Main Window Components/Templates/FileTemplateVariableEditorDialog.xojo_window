#tag Events ActionButton
	#tag Event
		Sub Pressed()
		  Var Mutable As Beacon.MutableFileTemplateVariable
		  If Self.mSourceVariable Is Nil Then
		    Var Type As Integer
		    Select Case Self.TypeMenu.SelectedRowIndex
		    Case Self.TypeIndexText
		      Type = Beacon.FileTemplateVariable.TypeText
		    Case Self.TypeIndexBoolean
		      Type = Beacon.FileTemplateVariable.TypeBoolean
		    Case Self.TypeIndexEnum
		      Type = Beacon.FileTemplateVariable.TypeEnum
		    End Select
		    Mutable = New Beacon.MutableFileTemplateVariable(Type)
		  Else
		    Mutable = New Beacon.MutableFileTemplateVariable(Self.mSourceVariable)
		  End If
		  
		  Var Label As String = Self.LabelField.Text.Trim
		  If Label.IsEmpty Then
		    Self.ShowAlert(Self.ErrorAlertTitle, Self.ErrorAlertBadLabel)
		    Return
		  End If
		  Mutable.Label = Label
		  
		  Var Name As String = Self.NameField.Text.Trim
		  If Name.IsEmpty Or Beacon.FileTemplateVariable.ValidateName(Name) = False Then
		    Self.ShowAlert(Self.ErrorAlertTitle, Self.ErrorAlertBadName)
		    Return
		  End If
		  Mutable.Name = Name
		  
		  Select Case Self.TypeMenu.SelectedRowIndex
		  Case Self.TypeIndexText
		    Var Validator As String = Self.ValidatorField.Text
		    If Validator.IsEmpty = False Then
		      Var Expression As New RegEx
		      Try
		        Expression.SearchPattern = Validator
		        Call Expression.Search("This Is A Test Value")
		        Mutable.RegexPattern = Validator
		      Catch Err As RuntimeException
		        Self.ShowAlert(Self.ErrorAlertTitle, Self.ErrorAlertBadRegex)
		        Return
		      End Try
		    End If
		  Case Self.TypeIndexEnum
		    If Self.OptionsList.RowCount < 1 Then
		      Self.ShowAlert(Self.ErrorAlertTitle, Self.ErrorAlertNoOptions)
		      Return
		    End If
		    
		    Var Options() As String
		    For Idx As Integer = 0 To Self.OptionsList.LastRowIndex
		      Options.Add(Self.OptionsList.CellTextAt(Idx, 0))
		    Next
		    Mutable.Options = Options
		  End Select
		  
		  Self.mCreatedVariable = Mutable.ImmutableVersion
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Pressed()
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events LabelField
	#tag Event
		Sub TextChanged()
		  If Self.mReady = False Then
		    Return
		  End If
		  
		  If Self.mNameHasBeenCustomized = False Then
		    Self.NameField.Text = Beacon.FileTemplateVariable.SuggestName(Me.Text)
		  End If
		  
		  Self.UpdateCodeExample()
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NameField
	#tag Event
		Sub TextChanged()
		  If Self.mReady = False Then
		    Return
		  End If
		  
		  If Self.Focus = Me Then
		    Self.mNameHasBeenCustomized = True
		  End If
		  
		  Self.UpdateCodeExample()
		End Sub
	#tag EndEvent
	#tag Event
		Sub FocusLost()
		  If Me.Text.Trim.IsEmpty Then
		    Self.mNameHasBeenCustomized = False
		    Me.Text = Beacon.FileTemplateVariable.SuggestName(Self.LabelField.Text)
		  Else
		    Me.Text = Beacon.FileTemplateVariable.SuggestName(Me.Text)
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TypeMenu
	#tag Event
		Sub SelectionChanged(item As DesktopMenuItem)
		  #Pragma Unused Item
		  
		  If Self.mReady = False Then
		    Return
		  End If
		  
		  Self.UpdateUI
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events OptionsList
	#tag Event
		Function CanCopy() As Boolean
		  Return Me.SelectedRowCount > 0
		End Function
	#tag EndEvent
	#tag Event
		Function CanDelete() As Boolean
		  Return Me.SelectedRowCount > 0
		End Function
	#tag EndEvent
	#tag Event
		Function CanEdit() As Boolean
		  Return Me.SelectedRowCount = 1
		End Function
	#tag EndEvent
	#tag Event
		Function CanPaste(Board As Clipboard) As Boolean
		  Return Board.TextAvailable
		End Function
	#tag EndEvent
	#tag Event
		Sub Opening()
		  Me.ColumnTypeAt(0) = DesktopListBox.CellTypes.TextField
		End Sub
	#tag EndEvent
	#tag Event
		Sub PerformClear(Warn As Boolean)
		  #Pragma Unused Warn
		  
		  For Idx As Integer = Me.LastRowIndex DownTo 0
		    If Me.RowSelectedAt(Idx) Then
		      Me.RemoveRowAt(Idx)
		    End If
		  Next
		End Sub
	#tag EndEvent
	#tag Event
		Sub PerformCopy(Board As Clipboard)
		  Var Options() As String
		  For Idx As Integer = 0 To Me.LastRowIndex
		    If Me.RowSelectedAt(Idx) Then
		      Options.Add(Me.CellTextAt(Idx, 0))
		    End If
		  Next
		  Board.Text = String.FromArray(Options, EndOfLine)
		End Sub
	#tag EndEvent
	#tag Event
		Sub PerformEdit()
		  Me.EditCellAt(Me.SelectedRowIndex, 0)
		End Sub
	#tag EndEvent
	#tag Event
		Sub PerformPaste(Board As Clipboard)
		  Var Options() As String = Board.Text.ReplaceLineEndings(EndOfLine).Split(EndOfLine)
		  For Each Option As String In Options
		    For Idx As Integer = 0 To Me.LastRowIndex
		      If Me.CellTextAt(Idx, 0).Compare(Option, ComparisonOptions.CaseSensitive) = 0 Then
		        Continue For Option
		      End If
		    Next
		    Me.AddRow(Option)
		  Next
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged()
		  Self.DeleteOptionButton.Enabled = Me.CanDelete
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events AddOptionButton
	#tag Event
		Sub Pressed()
		  Self.OptionsList.AddRow("New Option")
		  Self.OptionsList.SelectedRowIndex = Self.OptionsList.LastAddedRowIndex
		  Self.OptionsList.DoEdit
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DeleteOptionButton
	#tag Event
		Sub Pressed()
		  Self.OptionsList.DoClear
		End Sub
	#tag EndEvent
#tag EndEvents
