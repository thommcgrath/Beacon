#tag WindowCode
	#tag Event
		Sub Opening()
		  Self.SwapButtons()
		  
		  If (Self.mSourceVariable Is Nil) = False Then
		    Self.LabelField.Text = Self.mSourceVariable.Label
		    Self.NameField.Text = Self.mSourceVariable.Name
		    Select Case Self.mSourceVariable.Type
		    Case Beacon.FileTemplateVariable.TypeText
		      Self.TypeMenu.SelectedRowIndex = Self.TypeIndexText
		      Self.ValidatorField.Text = Self.mSourceVariable.RegexPattern
		    Case Beacon.FileTemplateVariable.TypeBoolean
		      Self.TypeMenu.SelectedRowIndex = Self.TypeIndexBoolean
		    Case Beacon.FileTemplateVariable.TypeEnum
		      Self.TypeMenu.SelectedRowIndex = Self.TypeIndexEnum
		      Var Options() As String = Self.mSourceVariable.Options
		      For Each Option As String In Options
		        Self.OptionsList.AddRow(Option)
		      Next
		    End Select
		    Self.TypeMenu.Enabled = False
		    Self.TitleLabel.Text = Self.EditTitleCaption
		    Self.mNameHasBeenCustomized = True
		  End If
		  
		  Self.mReady = True
		  Self.UpdateUI
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub Constructor(SourceVariable As Beacon.FileTemplateVariable)
		  If (SourceVariable Is Nil) = False Then
		    Self.mSourceVariable = SourceVariable.ImmutableVersion
		  End If
		  Super.Constructor
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Function Present(Parent As DesktopWindow, SourceVariable As Beacon.FileTemplateVariable = Nil) As Beacon.FileTemplateVariable
		  If (Parent Is Nil) = False Then
		    Parent = Parent.TrueWindow
		  End If
		  
		  Var Win As New FileTemplateVariableEditorDialog(SourceVariable)
		  Win.ShowModal(Parent)
		  Var CreatedVariable As Beacon.FileTemplateVariable = Win.mCreatedVariable
		  Win.Close
		  If (CreatedVariable Is Nil) = False Then
		    Return CreatedVariable.ImmutableVersion
		  End If
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateCodeExample()
		  Var CleanedName As String = Beacon.FileTemplateVariable.SuggestName(Self.NameField.Text)
		  If CleanedName.IsEmpty Then
		    CleanedName = Beacon.FileTemplateVariable.SuggestName(Self.LabelField.Text)
		    If CleanedName.IsEmpty Then
		      CleanedName = "ThisVariableHasNoName"
		    End If
		  End If
		  
		  Var ExamplePlaceholder As String = "${" + CleanedName + "}"
		  If Self.TypeMenu.SelectedRowIndex <> Self.TypeIndexBoolean Then
		    ExamplePlaceholder = "'" + ExamplePlaceholder + "'"
		  End If
		  Self.CodeExampleField.Text = "var myCustomVariable = " + ExamplePlaceholder + ";"
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateUI()
		  If Self.mReady = False Then
		    Return
		  End If
		  
		  Var ValidatorVisible As Boolean = Self.TypeMenu.SelectedRowIndex = Self.TypeIndexText
		  Var OptionsVisible As Boolean = Self.TypeMenu.SelectedRowIndex = Self.TypeIndexEnum
		  
		  Self.ValidatorLabel.Visible = ValidatorVisible
		  Self.ValidatorField.Visible = ValidatorVisible
		  Self.OptionsLabel.Visible = OptionsVisible
		  Self.OptionsList.Visible = OptionsVisible
		  Self.AddOptionButton.Visible = OptionsVisible
		  Self.DeleteOptionButton.Visible = OptionsVisible
		  
		  BeaconUI.SizeToFit(Self.CodeExampleLabel, Self.LabelLabel, Self.NameLabel, Self.OptionsLabel, Self.TypeLabel, Self.ValidatorLabel)
		  
		  Var FieldsLeft As Integer = Self.LabelLabel.Right + 12
		  Var FieldsWidth As Integer = Self.Width - (20 + FieldsLeft)
		  Var PreviousBottom As Integer
		  
		  Self.LabelField.Left = FieldsLeft
		  Self.LabelField.Width = FieldsWidth
		  Self.NameField.Left = FieldsLeft
		  Self.NameField.Width = FieldsWidth
		  Self.TypeMenu.Left = FieldsLeft
		  If ValidatorVisible Then
		    Self.ValidatorField.Left = FieldsLeft
		    Self.ValidatorField.Width = FieldsWidth
		    PreviousBottom = Self.ValidatorField.Bottom
		  Else
		    PreviousBottom = Self.TypeMenu.Bottom
		  End If
		  If OptionsVisible Then
		    Self.OptionsList.Top = PreviousBottom + 12
		    Self.AddOptionButton.Top = Self.OptionsList.Top
		    Self.DeleteOptionButton.Top = Self.AddOptionButton.Bottom + 12
		    Self.OptionsLabel.Top = Self.OPtionsList.Top
		    Self.OptionsList.Left = FieldsLeft
		    Self.OptionsList.Width = FieldsWidth - (12 + Self.AddOptionButton.Width)
		    Self.AddOptionButton.Left = Self.OptionsList.Right + 12
		    Self.DeleteOptionButton.Left = Self.AddOptionButton.Left
		    PreviousBottom = Self.OptionsList.Bottom
		  End If
		  Self.CodeExampleLabel.Top = PreviousBottom + 12
		  Self.CodeExampleField.Top = Self.CodeExampleLabel.Top
		  Self.CodeExampleField.Left = FieldsLeft
		  Self.CodeExampleField.Width = FieldsWidth
		  
		  Self.ActionButton.Top = Self.CodeExampleField.Bottom + 20
		  Self.CancelButton.Top = Self.ActionButton.Top
		  
		  Var Height As Integer = Self.ActionButton.Bottom + 20
		  Self.MaximumHeight = Height
		  Self.Height = Height
		  Self.MinimumHeight = Height
		  
		  Self.UpdateCodeExample()
		End Sub
	#tag EndMethod


