#tag WindowCode
	#tag Event
		Sub Opening()
		  Self.NameLabel.SizeToFit
		  Self.NameField.Left = Self.NameLabel.Right + 12
		  Self.NameField.Width = Self.Width - (20 + Self.NameField.Left)
		  
		  Self.NameField.Text = Self.mInitialName
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub Constructor(InitialName As String)
		  Self.mInitialName = InitialName
		  Super.Constructor
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Function Present(Parent As DesktopWindow, InitialName As String = "") As String
		  If (Parent Is Nil) = False Then
		    Parent = Parent.TrueWindow
		  End If
		  
		  Var Win As New ArkSAPlayerListPropertiesDialog(InitialName)
		  Win.ShowModal(Parent)
		  Var Name As String = Win.mChosenName
		  Win.Close
		  Return Name
		End Function
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mChosenName As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private mInitialName As String
	#tag EndProperty


#tag EndWindowCode

#tag Events ActionButton
	#tag Event
		Sub Pressed()
		  Self.mChosenName = Self.NameField.Text.Trim
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Pressed()
		  Self.mChosenName = ""
		  Self.Hide
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NameField
	#tag Event
		Sub TextChanged()
		  Self.ActionButton.Enabled = Self.NameField.Text.Trim.IsEmpty = False
		End Sub
	#tag EndEvent
#tag EndEvents
