#tag Events ConfigToolbar
	#tag Event
		Sub Opening()
		  Me.Append(OmniBarItem.CreateTitle("Title", Self.ConfigLabel))
		  Me.Append(OmniBarItem.CreateSeparator("TitleSeparator"))
		  Me.Append(OmniBarItem.CreateButton("AddList", "New List", IconToolbarAdd, "Create a new player list."))
		  Me.Append(OmniBarItem.CreateButton("Duplicate", "Duplicate", IconToolbarClone, "Duplicate the selected player list.", False))
		  Me.Append(OmniBarItem.CreateFlexibleSpace)
		  Me.Append(OmniBarItem.CreateHorizontalResizer("Resizer"))
		  
		  Me.Item("Title").Priority = 5
		  Me.Item("TitleSeparator").Priority = 5
		End Sub
	#tag EndEvent
	#tag Event
		Sub ItemPressed(Item As OmniBarItem, ItemRect As Rect)
		  #Pragma Unused ItemRect
		  
		  Select Case Item.Name
		  Case "AddList"
		    Self.ShowAddList()
		  Case "Duplicate"
		    Self.ShowDuplicateList()
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Sub Resize(DraggedResizer As OmniBarItem, DeltaX As Integer, DeltaY As Integer)
		  #Pragma Unused DraggedResizer
		  #Pragma Unused DeltaY
		  
		  Var NewWidth As Integer = Me.Width + DeltaX
		  Self.SetListWidth(NewWidth)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events FilterField
	#tag Event
		Sub TextChanged()
		  Self.UpdateList
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events List
	#tag Event
		Sub SelectionChanged()
		  Self.UpdateStatus()
		  
		  Var CloneButton As OmniBarItem = Self.ConfigToolbar.Item("Duplicate")
		  If (CloneButton Is Nil) = False Then
		    CloneButton.Enabled = Me.SelectedRowCount = 1
		  End If
		  
		  Select Case Me.SelectedRowCount
		  Case 0
		    Self.NoSelectionCanvas.Caption = "No Selection"
		    Self.Panel.SelectedPanelIndex = 0
		    Self.Editor.PlayerList = Nil
		  Case 1
		    Self.Panel.SelectedPanelIndex = 1
		    Self.Editor.PlayerList = Me.RowTagAt(Me.SelectedRowIndex)
		  Else
		    Self.NoSelectionCanvas.Caption = "Multiple Lists Selected"
		    Self.Panel.SelectedPanelIndex = 0
		    Self.Editor.PlayerList = Nil
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Function CanDelete() As Boolean
		  Return Me.SelectedRowCount > 0
		End Function
	#tag EndEvent
	#tag Event
		Function CanEdit() As Boolean
		  Return Me.SelectedRowCount = 1
		End Function
	#tag EndEvent
	#tag Event
		Sub PerformClear(Warn As Boolean)
		  Var Lists() As ArkSA.PlayerList
		  For Idx As Integer = 0 To Me.RowCount - 1
		    If Me.RowSelectedAt(Idx) = False Then
		      Continue
		    End If
		    
		    Var List As ArkSA.PlayerList = Me.RowTagAt(Idx)
		    If (List Is Nil) = False Then
		      Lists.Add(List)
		    End If
		  Next
		  
		  If Warn And Self.ShowDeleteConfirmation(Lists, "player list", "player lists") = False Then
		    Return
		  End If
		  
		  Var Config As ArkSA.Configs.PlayerLists = Self.Config(True)
		  For Each List As ArkSA.PlayerList In Lists
		    Config.Remove(List)
		  Next
		  Self.Modified = Self.Modified Or Config.Modified
		  Self.UpdateList()
		End Sub
	#tag EndEvent
	#tag Event
		Sub PerformEdit()
		  Var List As ArkSA.PlayerList = Me.RowTagAt(Me.SelectedRowIndex)
		  Var NewName As String = ArkSAPlayerListPropertiesDialog.Present(Self, List.Name)
		  If NewName.IsEmpty Then
		    Return
		  End If
		  
		  Try
		    Var Mutable As New ArkSA.MutablePlayerList(List)
		    Mutable.Name = NewName
		    
		    Var Config As ArkSA.Configs.PlayerLists = Self.Config(True)
		    Config.Add(Mutable)
		    Self.Modified = Self.Modified Or Config.Modified
		    Self.UpdateList(List)
		  Catch Err As RuntimeException
		    Self.ShowAlert("The list name is already in use", "This project already has a player list named '" + NewName + "' so you should pick another name.")
		    Return
		  End Try
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Editor
	#tag Event
		Sub ListChanged(List As ArkSA.PlayerList)
		  Var Config As ArkSA.Configs.PlayerLists = Self.Config(True)
		  Config.Add(List)
		  Self.List.RowTagAt(Self.List.SelectedRowIndex) = List
		  Self.Modified = Self.Modified Or Config.Modified
		End Sub
	#tag EndEvent
#tag EndEvents
