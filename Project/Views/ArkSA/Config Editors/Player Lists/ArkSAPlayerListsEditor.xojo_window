#tag WindowCode
	#tag Event
		Sub Resize(Initial As Boolean)
		  If Initial Then
		    Self.SetListWidth(Preferences.ArkSAPlayerListsSplitterPosition, False)
		  Else
		    Self.SetListWidth(Self.ConfigToolbar.Width)
		  End If
		  
		  Var Resizer As OmniBarItem = Self.ConfigToolbar.Item("Resizer")
		  If (Resizer Is Nil) = False Then
		    Resizer.Enabled = Self.Width > Self.MinEditorWidth
		  End If
		End Sub
	#tag EndEvent

	#tag Event
		Sub SetupUI()
		  Self.UpdateList()
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h1
		Protected Function Config(ForWriting As Boolean) As ArkSA.Configs.PlayerLists
		  Return ArkSA.Configs.PlayerLists(Super.Config(ForWriting))
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function InternalName() As String
		  Return ArkSA.Configs.NamePlayerLists
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Function MinEditorWidth() As Integer
		  Return ListMinWidth + ArkSAPlayerListEditor.MinEditorWidth + 1
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetListWidth(NewSize As Integer, Remember As Boolean = True)
		  Var ListWidth, EditorWidth As Integer
		  If Self.Width <= Self.MinEditorWidth Then
		    ListWidth = Self.ListMinWidth
		    EditorWidth = ArkSAPlayerListEditor.MinEditorWidth
		  Else
		    Var AvailableSpace As Integer = Self.Width - Self.ListSeparator.Width
		    ListWidth = Min(Max(NewSize, Self.ListMinWidth), AvailableSpace - ArkSAPlayerListEditor.MinEditorWidth)
		    EditorWidth = AvailableSpace - ListWidth
		  End If
		  
		  Self.ConfigToolbar.Width = ListWidth
		  Self.ListSeparator.Left = ListWidth
		  Self.List.Width = ListWidth
		  Self.Status.Width = ListWidth
		  Self.FilterSeparator.Width = ListWidth
		  Self.FilterField.Width = ListWidth - (Self.FilterField.Left * 2)
		  Self.Panel.Left = Self.ListSeparator.Left + Self.ListSeparator.Width
		  Self.Panel.Width = EditorWidth
		  
		  If Remember Then
		    Preferences.ArkSAPlayerListsSplitterPosition = ListWidth
		  End If
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowAddList()
		  Var ListName As String = ArkSAPlayerListPropertiesDialog.Present(Self)
		  If ListName.IsEmpty Then
		    Return
		  End If
		  
		  Try
		    Var Config As ArkSA.Configs.PlayerLists = Self.Config(True)
		    Var List As New ArkSA.PlayerList(ListName)
		    Config.Add(List)
		    Self.Modified = Self.Modified Or Config.Modified
		    Self.UpdateList(List)
		  Catch Err As RuntimeException
		    Self.ShowAlert("The list name is already in use", "This project already has a player list named '" + ListName + "' so you should pick another name.")
		    Return
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowDuplicateList()
		  Var List As ArkSA.PlayerList = Self.List.RowTagAt(Self.List.SelectedRowIndex)
		  Var NewName As String = ArkSAPlayerListPropertiesDialog.Present(Self, List.Name)
		  If NewName.IsEmpty Then
		    Return
		  End If
		  
		  Try
		    Var Clone As New ArkSA.PlayerList(NewName, List.Members)
		    Var Config As ArkSA.Configs.PlayerLists = Self.Config(True)
		    Config.Add(Clone)
		    Self.Modified = Self.Modified Or Config.Modified
		    Self.UpdateList(Clone)
		  Catch Err As RuntimeException
		    Self.ShowAlert("The list name is already in use", "This project already has a player list named '" + NewName + "' so you should pick another name.")
		    Return
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateList()
		  Var Lists() As ArkSA.PlayerList
		  For Row As Integer = 0 To Self.List.LastRowIndex
		    If Self.List.RowSelectedAt(Row) = False Then
		      Continue
		    End If
		    
		    Var List As ArkSA.PlayerList = Self.List.RowTagAt(Row)
		    If List Is Nil Then
		      Continue
		    End If
		    
		    Lists.Add(List)
		  Next
		  Self.UpdateList(Lists)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateList(SelectLists() As ArkSA.PlayerList)
		  Var SelectedListIds() As String
		  For Each List As ArkSA.PlayerList In SelectLists
		    SelectedListIds.Add(List.PlayerListId)
		  Next
		  
		  Var ListsByName As New Dictionary
		  Var Lists() As ArkSA.PlayerList = Self.Config(False).Lists
		  Var Names() As String
		  Var Filter As String = Self.FilterField.Text
		  For Each List As ArkSA.PlayerList In Lists
		    ListsByName.Value(List.Name) = List
		    If Filter.IsEmpty Or List.Name.Contains(Filter) Then
		      Names.Add(List.Name)
		    End If
		  Next
		  Names.Sort
		  
		  Self.List.SelectionChangeBlocked = True
		  Self.List.RowCount = Names.Count
		  For Row As Integer = 0 To Names.LastIndex
		    Var Name As String = Names(Row)
		    Var List As ArkSA.PlayerList = ListsByName.Value(Name)
		    Self.List.CellTextAt(Row, 0) = Name
		    Self.List.RowTagAt(Row) = List
		    Self.List.RowSelectedAt(Row) = SelectedListIds.IndexOf(List.PlayerListId) > -1
		  Next
		  Self.List.SelectionChangeBlocked = False
		  
		  Self.UpdateStatus()
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateList(SelectList As ArkSA.PlayerList)
		  Var Lists() As ArkSA.PlayerList
		  If (SelectList Is Nil) = False Then
		    Lists.Add(SelectList)
		  End If
		  Self.UpdateList(Lists)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateStatus()
		  If Self.List.SelectedRowCount > 0 Then
		    Self.Status.Caption = Self.List.SelectedRowCount.ToString(Locale.Current, "#,##0") + " of " + Language.NounWithQuantity(Self.List.RowCount, "List", "Lists") + " Selected"
		  Else
		    Self.Status.Caption = Language.NounWithQuantity(Self.List.RowCount, "List", "Lists")
		  End If
		End Sub
	#tag EndMethod


