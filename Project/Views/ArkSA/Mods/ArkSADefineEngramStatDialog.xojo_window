#tag WindowCode
	#tag Event
		Sub Opening()
		  Self.StatLabel.Text = Self.FieldLabelStat + ":"
		  Self.IVCLabel.Text = Self.FieldLabelIVC + ":"
		  Self.RRMLabel.Text = Self.FieldLabelRRM + ":"
		  Self.RROLabel.Text = Self.FieldLabelRRO + ":"
		  Self.RVMLabel.Text = Self.FieldLabelRVM + ":"
		  Self.SMSLabel.Text = Self.FieldLabelSMS + ":"
		  Self.AMVLabel.Text = Self.FieldLabelAMV + ":"
		  BeaconUI.SizeToFit(Self.StatLabel, Self.IVCLabel, Self.RRMLabel, Self.RROLabel, Self.RVMLabel, Self.SMSLabel, Self.AMVLabel)
		  
		  Var FieldLeft As Integer = Self.StatLabel.Right + 12
		  Var FieldWidth As Integer = Self.Width - (20 + FieldLeft)
		  Self.StatMenu.Left = FieldLeft
		  Self.StatMenu.Width = FieldWidth
		  Var Fields() As DesktopUIControl = Array(Self.IVCField, Self.RRMField, Self.RROField, Self.RVMField, Self.SMSField, Self.AMVField)
		  For Idx As Integer = 0 To Fields.LastIndex
		    Fields(Idx).Left = FieldLeft
		    Fields(Idx).Width = FieldWidth
		  Next
		  
		  For StatIndex As Integer = ArkSA.EngramStat.FirstIndex To ArkSA.EngramStat.LastIndex
		    Var Mask As Integer = ArkSA.EngramStat.MaskForIndex(StatIndex)
		    If (Mask And Self.mAlreadyUsedStats) > 0 Then
		      Continue
		    End If
		    
		    Self.StatMenu.AddRow(ArkSA.EngramStat.LabelForIndex(StatIndex), StatIndex)
		    If (Self.mStat Is Nil) = False And Self.mStat.StatIndex = StatIndex Then
		      Self.StatMenu.SelectedRowIndex = Self.StatMenu.LastAddedRowIndex
		    End If
		  Next
		  If (Self.mStat Is Nil) = False Then
		    Self.StatMenu.Enabled = False
		    Self.IVCField.Text = Self.mStat.InitialValueConstant.PrettyText(True)
		    Self.RRMField.Text = Self.mStat.RandomizerRangeMultiplier.PrettyText(True)
		    Self.RROField.Text = Self.mStat.RandomizerRangeOverride.PrettyText(True)
		    Self.RVMField.Text = Self.mStat.RatingValueMultiplier.PrettyText(True)
		    Self.SMSField.Text = Self.mStat.StateModifierScale.PrettyText(True)
		    Self.AMVField.Text = Self.mStat.AbsoluteMaxValue.PrettyText(True)
		  End If
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub Constructor(AlreadyUsedStats As Integer, Stat As ArkSA.EngramStat)
		  Self.mStat = Stat
		  Self.mAlreadyUsedStats = AlreadyUsedStats
		  Super.Constructor
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Function Present(Parent As DesktopWindow, AlreadyUsedStats As Integer, Stat As ArkSA.EngramStat = Nil) As ArkSA.EngramStat
		  If Parent Is Nil Then
		    Return Nil
		  End If
		  
		  Var Win As New ArkSADefineEngramStatDialog(AlreadyUsedStats, Stat)
		  Win.ShowModal(Parent)
		  
		  Var NewStat As ArkSA.EngramStat
		  If Win.mCancelled = False Then
		    NewStat = Win.mStat
		  End If
		  Win.Close
		  
		  Return NewStat
		End Function
	#tag EndMethod


