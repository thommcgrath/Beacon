#tag Events DayMultiplierField
	#tag Event
		Sub TextChanged()
		  If Self.SettingUp Or IsNumeric(Me.Value) = False Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Config(True).DaySpeedMultiplier = CDbl(Me.Value)
		  Self.Changed = True
		  Self.UpdateCalculations()
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NightMultiplierField
	#tag Event
		Sub TextChanged()
		  If Self.SettingUp Or IsNumeric(Me.Value) = False Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Config(True).NightSpeedMultiplier = CDbl(Me.Value)
		  Self.Changed = True
		  Self.UpdateCalculations()
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DayLengthField
	#tag Event
		Sub TextChanged()
		  If Self.SettingUp Or IsNumeric(Me.Value) = False Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Dim DesiredLength As Double = Max(CDbl(Me.Value), 0.016666667) // At least one second
		  Self.Config(True).DaySpeedMultiplier = Self.mOfficialDaySeconds / (DesiredLength * 60)
		  Self.Changed = True
		  Self.UpdateCalculations()
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NightLengthField
	#tag Event
		Sub TextChanged()
		  If Self.SettingUp Or IsNumeric(Me.Value) = False Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Dim DesiredLength As Double = Max(CDbl(Me.Value), 0.016666667) // At least one second
		  Self.Config(True).NightSpeedMultiplier = Self.mOfficialNightSeconds / (DesiredLength * 60)
		  Self.Changed = True
		  Self.UpdateCalculations()
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
